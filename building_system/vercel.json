{
    "version": 2,
    "builds": [
    {
        "src": "building_system/wsgi.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.11" }
    }
    ],
    "routes": [
    { 
        "src": "/(.*[^/])", 
        "status": 308, 
        "headers": { 
        "Location": "$1/" 
        } 
    },
    {
        "src": "/(.*)",
        "dest": "building_system/wsgi.py"
    }
    ]
}